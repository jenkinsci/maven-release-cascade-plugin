<!--
	Action failed due to cascade member projects being active. 

	@author Andrei Pozolotin
-->
<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler"
	xmlns:d="jelly:define" xmlns:l="/lib/layout" xmlns:t="/lib/hudson"
	xmlns:f="/lib/form">

	<l:layout>

		<l:main-panel>

			<f:section>

				<img title="Cascade Release" width="48" height="48"
					src="${it.PLUGIN_IMAGES_48}/dragon.png" />

			</f:section>

			<h1>Can not schedule cascade release.</h1>

			<p />

			<h2>Cascade family projects are active:</h2>

			<ul style="list-style-type: none;">
				<j:forEach var="entry"
					items="${it.reportActiveFamilyProjects().entrySet()}">

					<li><img src="${it.PLUGIN_IMAGES_16}/help.png" alt="" /> <a
						href="${entry.value.projectUrl()}" class="model-link">
							${entry.key} </a></li>

				</j:forEach>
			</ul>

		</l:main-panel>

	</l:layout>

</j:jelly>
